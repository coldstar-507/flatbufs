namespace flatgen;

table MessageId {
 timestamp:int64;
 u32:uint32;
 root:Root;
 kind:uint8;
}

table MediaId {
 timestamp:int64;
 u32:uint32;
 place:uint16;
 width:uint16;
 height:uint16;
 squared:bool;
 video:bool;
 kind:uint8;
}

table NodeId {
 timestamp:int64;
 u32:uint32;
 kind:uint8;
}

table Root {
 primary:NodeId;
 secondary:NodeId;
 timestamp:int64;
 chat_place:uint16;
 kind:uint8;
}

table TempId {
 timestamp:int64;
 u32:uint32;
 place:uint16;
 kind:uint8;
}

table PushId {
 u32:uint32;
 timestamp:int64;
 node_id:NodeId;
 device:uint32;
 kind:uint8;
}

table MessageTarget {
  // user_id:string;
  // device_id:uint32;
 token:string;
 show_notif:bool;
}

table Notifications {
 sender:string;
 root:string;
 header:string;
 body:string;
 targets:[MessageTarget];
}
root_type Notifications;


table PushRequest {
 push_id:PushId;
 payload:[ubyte];
}
root_type PushRequest;


table MediaMetadata {
 media_id:MediaId;
 temp_id:TempId;  
 owner_id:string;
 timestamp:int64;
 mime:string;
 is_reversed:bool;
 is_encrypted:bool;
 is_paid_to_view:bool;
 is_paid_to_own:bool;
 is_locked:bool;
 is_saved:bool;
}

root_type MediaMetadata;

table FullMedia {
 metadata:MediaMetadata;
 data:[ubyte];
}

root_type FullMedia;

struct Offset {
 x:float;
 y:float;
}

table Sticker {
 media_id:string;
 temp:string;
 pos:Offset;
 size:Offset;
 rotation:float;
 scale:float;
 emoji:string;
}

table MessageEvent {
 chat_id:MessageId;
 type:ubyte;
 reaction_id:string;
 sender_id:string;
 sender_tag:string;
 message_id:string;
 root:string;
 tag:string;
 timestamp:uint64;
 forwarded_from:string;
 forwarded_from_tag:string;
 payment_id:string;
 nodes:[string];
 replies:[string];
 txt:string;
 media_id:string;
 temp_media:string;
 temp_payment:string;
 emoji:string;
 sticks:[Sticker];
 snip_size:Offset;
}

root_type MessageEvent;


